<h1>Listing filing_categories</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Std Deduction</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @filing_categories.each do |filing_category| %>
  <tr>
    <td><%= filing_category.name %></td>
    <td><%= filing_category.description %></td>
    <td>$<%= filing_category.standard_deduction %></td>
    <td><%= link_to 'Show', filing_category %></td>
    <td><%= link_to 'Edit', edit_filing_category_path(filing_category) %></td>
    <td><%= link_to 'Destroy', filing_category, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

